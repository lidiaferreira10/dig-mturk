@prefix ht: <http://memexproxy.com/ontology/human-trafficking/> . 
@prefix memex: <http://memexproxy.com/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix schema: <http://schema.org/> .
@prefix mturk: <http://dig.isi.edu/ontology/mturk/> .

<http://dig.isi.edu/ontology/mturk/> rdf:type owl:Ontology .

#########################################################
#
# Classes
#
#########################################################

mturk:Annotation rdf:type owl:Class ; 
	rdfs:subClassOf schema:CreativeWork ;
	rdfs:label "Annotation"@en ;
	rdfs:comment """Document (Creative Work) produced by annotating part of another CreativeWork"""
	.

mturk:annotationOffset rdf:type owl:DatatypeProperty ;
	rdfs:label "annotationOffset"@en ;
	rdfs:domain mturk:Annotation ;
	rdfs:comment "The beginning offset for an annotation"
	.

mturk:annotationLabel rdf:type owl:DatatypeProperty ;
	rdfs:label "annotationLabel"@en ;
	rdfs:domain mturk:Annotation ;
	rdfs:comment "The label for an annotation"
	.

mturk:annotationValue rdf:type owl:DatatypeProperty ;
	rdfs:label "annotationValue"@en ;
	rdfs:domain mturk:Annotation ;
	rdfs:comment "The value for an annotation"
	.
